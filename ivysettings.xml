<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ivysettings>
<!-- generic AcceptAllDTD to make the Eclipse Warning go away -->
<ivysettings>
    <property name="ivy.repo.url" value="http://www.sosy-lab.org/ivy"/>
    <property name="ivy.repo.url2" value=""/>
    <settings defaultResolver="all"/>
    <resolvers>
        <chain name="all">
          <ibiblio name="maven" m2compatible="true" root="https://repo1.maven.org/maven2/"/>
          <url name="Sosy-Lab" descriptor="required">
              <ivy pattern="${ivy.repo.url}/[organisation]/[module]/ivy-[revision].xml"/>
              <artifact pattern="${ivy.repo.url}/[organisation]/[module]/[artifact]-[revision](-[classifier]).[ext]"/>
          </url>
        </chain>
    </resolvers>
</ivysettings>
